{
  "name": "pi-view-port",
  "version": "0.1.1",
  "description": "A Vue 3 component for previewing files.",
  "main": "./dist/pi-view-port.umd.js",
  "module": "./dist/pi-view-port.es.js",
  "types": "./dist/pi-view-port.d.ts",
  "exports": {
    ".": {
      "types": "./dist/pi-view-port.d.ts",
      "import": "./dist/pi-view-port.es.js",
      "require": "./dist/pi-view-port.umd.js"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist",
    "public"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "keywords": [
    "vue",
    "component",
    "file",
    "preview",
    "pdf",
    "markdown",
    "html",
    "image",
    "video",
    "audio",
    "office"
  ],
  "author": "pi",
  "license": "ISC",
  "dependencies": {
    "dompurify": "^3.3.1",
    "fs-extra": "^11.3.3",
    "highlight.js": "^11.11.1",
    "lucide-vue-next": "^0.511.0",
    "marked": "^17.0.1",
    "nanoid": "^5.1.6",
    "pdfjs-dist": "^5.4.624",
    "serve-static": "^2.2.1",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/dompurify": "^3.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/marked": "^5.0.2",
    "@types/node": "^22.15.30",
    "@types/pdfjs-dist": "^2.10.377",
    "@types/serve-static": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/runtime-dom": "^3.4.15",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "jsdom": "^28.0.0",
    "sass": "^1.97.3",
    "typescript": "~5.3.3",
    "unplugin-vue-dev-locator": "^1.0.0",
    "vite": "^5.0.12",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^1.8.27"
  }
}
